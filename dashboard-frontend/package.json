{
  "name": "kpi-dashboard",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "node server.js",
    "start:email": "node server-email.js",
    "start:oauth": "node server-oauth.js",
    "start:ldap": "node server-ldap.js",
    "start:scim": "node server-scim.js",
    "start:saml": "node server-saml.js",
    "start:datasources": "node server-datasources.js",
    "start:etl": "node server-etl.js",
    "start:bookkeeping": "node server-bookkeeping.js",
    "start:all": "concurrently \"npm start\" \"npm run start:server\" \"npm run start:email\" \"npm run start:oauth\" \"npm run start:ldap\" \"npm run start:scim\" \"npm run start:saml\" \"npm run start:datasources\" \"npm run start:etl\" \"npm run start:bookkeeping\"",
    "build": "ng build",
    "vercel-build": "npm install && ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "@mariomuja/angular-shared-components": "file:./mario-muja-angular-shared-components-1.0.6.tgz",
    "chart.js": "^4.4.0",
    "decimal.js": "^10.4.3",
    "jspdf": "^3.0.3",
    "marked": "^16.4.1",
    "ng2-charts": "^5.0.0",
    "pg": "^8.11.3",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.0",
    "@angular/cli": "^17.3.0",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "^5.1.0",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.11.0",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "typescript": "~5.2.2"
  }
}
